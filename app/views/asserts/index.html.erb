<p id="notice"><%= notice %></p>

<h1>Asserts</h1>

<table>
  <thead>
    <tr>
      <th>Equipment name</th>
      <th>Equipment price</th>
      <th>Company detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asserts.each do |assert| %>
      <tr>
        <td><%= assert.equipment_name %></td>
        <td><%= assert.equipment_price %></td>
        <td><%= assert.company_detail %></td>
        <td><%= link_to 'Show', assert %></td>
        <td><%= link_to 'Edit', edit_assert_path(assert) %></td>
        <td><%= link_to 'Destroy', assert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assert', new_assert_path %>
